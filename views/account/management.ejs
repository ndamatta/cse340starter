<% if (title) { %>
    <h1 id="account-manag-title"><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

<%- messages() %>
<% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>
<div id='account-manag-display'>
  <table id="accountTable"><%- accountList %></table>
  <noscript>JavaScript must be enabled to use this page.</noscript>
</div>
<script src='/js/script.js'></script>
<script src='/js/account.js'></script>